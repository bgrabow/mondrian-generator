{
  "expo": {
    "name": "Mondrian Generator",
    "slug": "mondrian-generator",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "me.bgrabow.mondriangenerator",
      "supportsTablet": true
    },
    "android": {
      "package": "me.bgrabow.mondrian_generator"
    }
  }
}